{
  "name": "@tsbuffer/proto-generator",
  "version": "3.0.0-dev.0",
  "description": "",
  "main": "./dist/index.js",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "npx mocha test/**/*.test.ts",
    "coverage": "nyc mocha test/**/*.test.ts && start coverage\\index.html",
    "dev": "rollup -c -w",
    "check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts --fix",
    "build": "shx rm -rf dist && rollup -c"
  },
  "author": "k8w",
  "keywords": [
    "tsbuffer",
    "protobuf",
    "typescript",
    "serialize",
    "json",
    "schema",
    "binary",
    "protocol",
    "websocket",
    "k8w"
  ],
  "license": "MIT",
  "dependencies": {
    "@tsbuffer/schema": "workspace:^",
    "k8w-crypto": "^0.2.0",
    "tslib": "*"
  },
  "devDependencies": {
    "@types/mocha": "^8.2.3",
    "@types/node": "^15.14.9",
    "k8w-extend-native": "^1.4.6",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "k8w-extend-native": "^1.4.6"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "coverage",
      "test"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}